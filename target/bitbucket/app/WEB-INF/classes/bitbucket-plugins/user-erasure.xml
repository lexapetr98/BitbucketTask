<atlassian-plugin key="com.atlassian.bitbucket.server.bitbucket-user-erasure"
                  name="Bitbucket User Anonymization Handlers">

    <plugin-info>
        <description>Bitbucket's User Anonymization Handlers</description>
        <version>1.0</version>
        <vendor name="Atlassian" url="http://www.atlassian.com"/>
    </plugin-info>

    <user-erasure-handler key="project" class="com.atlassian.stash.internal.project.PersonalProjectUserErasureHandler" />
    <user-erasure-handler key="commentMentions" class="com.atlassian.stash.internal.notification.mention.CommentMentionUserErasureHandler" />
    <user-erasure-handler key="pullRequestMentions" class="com.atlassian.stash.internal.notification.mention.PullRequestMentionUserErasureHandler" />
</atlassian-plugin>

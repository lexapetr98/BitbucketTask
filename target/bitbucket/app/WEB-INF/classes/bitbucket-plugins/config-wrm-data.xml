<atlassian-plugin key="com.atlassian.bitbucket.server.config-wrm-data" name="Config WRM Data">

    <!-- When adding a property listing here, follow the convention: key is the property key, data key is "data" -->

    <web-resource key="attachment.upload.max.size" name="attachment.upload.max.size">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.AttachmentUploadMaxSizeDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="dashboard.poll.pull-request-suggestions.interval" name="dashboard.poll.pull-request-suggestions.interval">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.DashboardPollPullRequestSuggestionsIntervalDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="dashboard.poll.pull-requests.interval" name="dashboard.poll.pull-requests.interval">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.DashboardPollPullRequestsIntervalDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="display.max.source.lines" name="display.max.source.lines">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.DisplayMaxSourceLinesDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="git.pull-request.rebase.timeout" name="git.pull-request.rebase.timeout">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.GitRebaseTimeoutDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="repository.merge.sync.timeout" name="repository.merge.sync.timeout">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.RefSyncMergeTimeoutDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="repository.rebase.sync.timeout" name="repository.rebase.sync.timeout">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.RefSyncRebaseTimeoutDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="page.max.source.lines" name="page.max.source.lines">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.PageMaxSourceLinesDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="page.max.diff.lines" name="page.max.diff.lines">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.PageMaxDiffLinesDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="page.max.directory.recursive.children" name="page.max.directory.recursive.children">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.PageMaxDirectoryRecursiveChildrenDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

    <web-resource key="content.upload.max.size" name="content.upload.max.size">
        <data key="data" class="com.atlassian.stash.internal.web.fragments.ContentUploadMaxUploadSizeDataProvider"/>
        <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:data</dependency>
    </web-resource>

</atlassian-plugin>

{namespace bitbucket.internal.feature.alerts}

/**
 */
{template .emptyTrigger}
    <span class="aui-icon aui-icon-small aui-icon-success"></span>
{/template}

/**
 * @param title
 * @param mostSevereType
 * @param count
 */
{template .trigger}
    <div>
        <span class="aui-icon aui-icon-small aui-iconfont-{$mostSevereType}">{$title}</span>
        <span class="alerts-count">{$count}</span>
    </div>
{/template}

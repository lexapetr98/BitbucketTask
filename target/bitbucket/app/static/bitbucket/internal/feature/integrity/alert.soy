{namespace bitbucket.notification.alert}

/**
 * @param? integrityCheckState the current state of the integrity checks
 */
{template .bitbucketIntegrityCheckRunning}
    {if $integrityCheckState}
        <script type="text/javascript">
            require('bitbucket/internal/feature/integrity/alert').init({to_json($integrityCheckState)|noAutoescape});
        </script>
    {/if}
{/template}